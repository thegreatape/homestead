{
  "background": {
    "scripts": ["background.js"]
  },
  "description": "",
  "icons": {
    "48": "icons/history.svg"
  },
  "permissions": [
    "activeTab",
    "history",
    "tabs"
  ],

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],

  "manifest_version": 2,
  "name": "Homestead",
  "version": "1.0",

  "options_ui": {
    "page": "options.html"
  },

  "permissions": ["storage"],

  "browser_specific_settings": {
    "gecko": {
      "id": "homestead@thegreata.pe"
    }
  }
}
